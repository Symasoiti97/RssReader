<Application x:Class="WpfAppRss.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfAppRss"
             StartupUri="Views/LoginWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Styles\Resources.xaml" />
                
                <ResourceDictionary Source="Styles\Texts.xaml" />
                <ResourceDictionary Source="Styles\Buttons.xaml" />
                
                <ResourceDictionary Source="Styles\Windows\WindowStyle.xaml" />
                
                <ResourceDictionary Source="Styles\TreeViews.xaml" />
                <ResourceDictionary Source="Styles\ListBoxes.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
